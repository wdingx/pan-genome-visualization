{
  "name": "panX-visualization",
  "version": "1.5.0",
  "description": "pan-genome-visualization",
  "private": true,
  "scripts": {
    "clean": "rm -rf public/*.html public/*.html.gz public/dist/*",
    "build": "NODE_ENV=production npm run build:webpack && NODE_ENV=production npm run build:gulp && NODE_ENV=production npm run build:html",
    "build:webpack": "NODE_ENV=production webpack --env production",
    "build:gulp": "NODE_ENV=production gulp",
    "build:html": "rm -rf public/*.html && node ./scripts/renderPageHtml.js && node ./scripts/renderPathogenHtml.js",
    "start": "NODE_ENV=production serve --cors --listen=tcp://0.0.0.0:8000 --no-clipboard --no-port-switching public/"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "bootstrap-multiselect": "0.9.13-1",
    "bootstrap-toggle": "2.2.2",
    "chroma-js": "1.2.2",
    "compression": "1.6.2",
    "cookie-parser": "1.3.5",
    "crossfilter": "1.3.12",
    "d3": "3.5.17",
    "d3-tip": "0.7.1",
    "datatable": "2.0.2",
    "datatables.net": "1.10.13",
    "datatables.net-colreorder": "1.3.3",
    "datatables.net-dt": "1.10.13",
    "dc": "2.1.3",
    "debug": "2.2.0",
    "import-loader": "1.0.1",
    "jquery": "3.2.1",
    "jquery.panzoom": "3.2.2",
    "msa": "1.0.3",
    "nouislider": "9.2.0",
    "require-dir": "0.3.2",
    "svg-pan-zoom": "3.5.0"
  },
  "devDependencies": {
    "allow-methods": "4.1.1",
