  body
    include navbar
    .container
      //- .row
      //- img(src='images/pan-blue-noReads.svg', style='width:30px;height:30px;')
      .grid-stack
        br
        br
        //- dc-charts(tool A)
        //- data-count(tool A)
        #dc_data_count.col-md-6.grid-stack-item(data-gs-x='0', data-gs-y='1', data-gs-width='6', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-info(role='alert') count
          .col-md-7
            .dc_data_count
          .col-md-2
            span#dc_coreAcc_piechart
          .col-md-2
            span#coreThreshold(data-toggle='tooltip', data-placement='bottom', title='coreThreshold ')
              | cutoff:0.99
            #changeCoreThreshold
        //- straincount-chart(tool A)
        #dc_straincount_chart.col-md-3.grid-stack-item(data-gs-x='0', data-gs-y='3', data-gs-width='3', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-info(role='alert') Gene count rank distribution
        //- geneLength-chart(tool A)
        #dc_geneLength_chart.col-md-3.grid-stack-item(data-gs-x='3', data-gs-y='3', data-gs-width='3', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-info(role='alert') Gene length distribution

        //- dc-charts(tool B)
        //- data-count(tool B)
        #dc_data_count2.col-md-6.grid-stack-item(data-gs-x='6', data-gs-y='1', data-gs-width='6', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-warning(role='alert') count
          .col-md-7
            .dc_data_count2
          .col-md-2
            span#dc_coreAcc_piechart2
          .col-md-2
            span#coreThreshold2(data-toggle='tooltip', data-placement='bottom', title='coreThreshold ')
              | cutoff:0.99
            #changeCoreThreshold2
        //- straincount-chart(tool B)
        #dc_straincount_chart2.col-md-3.grid-stack-item(data-gs-x='6', data-gs-y='3', data-gs-width='3', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-warning(role='alert') Gene count rank distribution
        //- geneLength-chart(tool B)
        #dc_geneLength_chart2.col-md-3.grid-stack-item(data-gs-x='9', data-gs-y='3', data-gs-width='3', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-warning(role='alert') Gene length distribution

        //- cluster table(tool A)
        .col-md-6.grid-stack-item(data-gs-x='0', data-gs-y='7', data-gs-width='6', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-info(role='alert') Gene cluster table
          table#dc_data_table1.table.table-striped.table(cellspacing='0', width='100%')
        //- cluster table(tool B)
        .col-md-6.grid-stack-item(data-gs-x='6', data-gs-y='7', data-gs-width='6', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-warning(role='alert') Gene cluster table
          table#dc_data_table2.table.table-striped.table(cellspacing='0', width='100%')

        //- //- MSA viewer
        //- .col-md-6.grid-stack-item(data-gs-x='6', data-gs-y='5', data-gs-width='6', data-gs-height='0')
        //-   .grid-stack-item-content
        //-   .alert.alert-info(role='alert')
        //-     #sequence_viewer_title Sequence alignment
        //-   #snippetDiv

        //- strain tree (tool A)
        .col-md-6.grid-stack-item(data-gs-x='0', data-gs-y='14', data-gs-width='6', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-info(role='alert')
            | Strain tree (SNPs in all core genes)
          .row
            #LeftTreeButtons.col-md-5(style='vertical-align: top')
              input#TreeViewSelect_01.btn_tooltip.input(type='checkbox', data-toggle='toggle', data-style='ios', data-on='Circular', data-off='Vertical', data-size='mini', data-onstyle='primary', data-offstyle='info', text='layout')
              input#LabelsToggle_01.btn_tooltip(type='checkbox', checked='', data-toggle='toggle', data-style='ios', data-on='Label', data-off='Label', data-size='mini')
              input#InnerNodeToggle_01.btn_tooltip(type='checkbox', checked='', data-toggle='toggle', data-style='ios', data-on='Node', data-off='Node', data-size='mini')
              input#ScalesToggle_01.btn_tooltip(type='checkbox', checked='', data-toggle='toggle', data-style='ios', data-on='Scale', data-off='Scale', data-size='mini')
              button#Height_plus_Toggle_01.btn.btn-xs.btn-primary.btn_tooltip
                span.glyphicon.glyphicon-resize-full
              button#Height_minus_Toggle_01.btn.btn-xs.btn-primary.btn_tooltip
                span.glyphicon.glyphicon-resize-small
            #tree_zoom_01.col-md-2.btn_tooltip
              input#tree_zoom_range_01.col-md-10(type='range', style='padding:0;')
              button#tree_zoom_reset_01.btn.btn-xs.btn-primary.btn_tooltip
                span.glyphicon.glyphicon-refresh
            #dropdown_list_01.col-md-3.btn_tooltip
            #download_coreTree_01.col-md-1.btn_tooltip
          .row
            .col-md-2
              br
              br
              br
              svg#legend
            .col-md-10
              #mytree1
        //- gene tree (tool A)
        .col-md-6.grid-stack-item(data-gs-x='0', data-gs-y='26', data-gs-width='6', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-info(role='alert')
            #genetree_title_01 Gene tree (individual gene)
          .row
            .col-md-4
              #searchStrain-group_01.input-group
                span#sizing-addon_01.input-group-addon.primary.sm-customized Search strain
                input.form-control.sm-customized(type='text', placeholder='NCBI accession number', aria-describedby='sizing-addon_01',oninput='search(this.value)')
            #tree_rotate_div_01.col-md-2
              input#tree_rotate_01(type='checkbox', data-toggle='toggle', data-style='ios', data-on='left-right', data-off='right-left', data-size='mini', data-onstyle='primary', data-offstyle='info')
            //#dropdown_list2.col-md-4
            #download_geneTree_01.col-md-1.btn_tooltip
          .row
            .col-md-10
              #mytree2
            .col-md-2
              br
              br
              br
              svg#legend2

        //- strain tree (tool B)
        .col-md-6.grid-stack-item(data-gs-x='6', data-gs-y='14', data-gs-width='6', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-warning(role='alert')
            | Strain tree (SNPs in all core genes)
          .row
            #LeftTreeButtons.col-md-5(style='vertical-align: top')
              input#TreeViewSelect_02.btn_tooltip.input(type='checkbox', data-toggle='toggle', data-style='ios', data-on='Circular', data-off='Vertical', data-size='mini', data-onstyle='primary', data-offstyle='info', text='layout')
              input#LabelsToggle_02.btn_tooltip(type='checkbox', checked='', data-toggle='toggle', data-style='ios', data-on='Label', data-off='Label', data-size='mini')
              input#InnerNodeToggle_02.btn_tooltip(type='checkbox', checked='', data-toggle='toggle', data-style='ios', data-on='Node', data-off='Node', data-size='mini')
              input#ScalesToggle_02.btn_tooltip(type='checkbox', checked='', data-toggle='toggle', data-style='ios', data-on='Scale', data-off='Scale', data-size='mini')
              button#Height_plus_Toggle_02.btn.btn-xs.btn-primary.btn_tooltip
                span.glyphicon.glyphicon-resize-full
              button#Height_minus_Toggle_02.btn.btn-xs.btn-primary.btn_tooltip
                span.glyphicon.glyphicon-resize-small
            #tree_zoom_02.col-md-2.btn_tooltip
              input#tree_zoom_range_02.col-md-10(type='range', style='padding:0;')
              button#tree_zoom_reset_02.btn.btn-xs.btn-primary.btn_tooltip
                span.glyphicon.glyphicon-refresh
            #dropdown_list_02.col-md-3.btn_tooltip
            #download_coreTree_02.col-md-1.btn_tooltip
          .row
            .col-md-2
              br
              br
              br
              svg#legend
            .col-md-10
              #compare_tree1
        //- gene tree (tool B)
        .col-md-6.grid-stack-item(data-gs-x='6', data-gs-y='26', data-gs-width='6', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-warning(role='alert')
            #genetree_title_02 Gene tree (individual gene)
          .row
            .col-md-4
              #searchStrain-group_02.input-group
                span#sizing-addon_02.input-group-addon.primary.sm-customized Search strain
                input.form-control.sm-customized(type='text', placeholder='NCBI accession number', aria-describedby='sizing-addon_02',oninput='search(this.value)')
            #tree_rotate_div_02.col-md-2
              input#tree_rotate_02(type='checkbox', data-toggle='toggle', data-style='ios', data-on='left-right', data-off='right-left', data-size='mini', data-onstyle='primary', data-offstyle='info')
            //#dropdown_list2.col-md-4
            #download_geneTree_02.col-md-1.btn_tooltip
          .row
            .col-md-10
              #compare_tree2
            .col-md-2
              br
              br
              br
              svg#legend2

        //- metadata table
        .col-md-12.grid-stack-item(data-gs-x='0', data-gs-y='32', data-gs-width='12', data-gs-height='0')
          .grid-stack-item-content
          .alert.alert-info(role='alert') Strain meta-info table
          table#dc_data_table_meta.table.table-striped.table(cellspacing='0', width='100%')

      .row
        .row
          .row
            .row
              .row
          br
          br
          br
          br
          br
          br
          br
          br
          br
          br
          br
          br
          br
    include footer
    block content
